{% extends 'GremioAbmBundle::layout.html.twig' %}
{% block body %}
<div class="mws-panel grid_6">
                	<div class="mws-panel-header">
                    	<span class="mws-i-24 i-table-1">Lista de Tipos de Socio</span>
                    </div>
                    <div class="mws-panel-body">
                        <div class="mws-panel-toolbar bottom clearfix">
                        	<ul>
                            	<li><a href="{{ path('administracion_entidades_tiposocio_new') }}" class="mws-ic-16 ic-add">Alta de Tipo de Socio</a></li>
                            	
                            </ul>
                        </div>
                            <div>
                {% if app.session.hasFlash('succes') %}

                    <div class="mws-form-message success">
                        {{ app.session.flash('succes') }}
                    </div>
                {% endif %}
            </div>

<table class="mws-datatable-fn mws-table">
    <thead>
        <tr>
           {# <th>Id</th> #}
            <th>Descripcion</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr href="{{ url('administracion_entidades_tiposocio_show', { 'id': entity.id }) }}" class="dbclickeable">
            {# <td><a href="{{ path('administracion_entidades_tiposocio_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td> #}
            <td>{{ entity.descripcion }}</td>
            
        </tr>
    {% endfor %}
    </tbody>
</table>
</div>
</div>
    
{% endblock %}

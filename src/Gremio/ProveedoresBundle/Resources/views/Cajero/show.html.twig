{% extends 'GremioAbmBundle::layout.html.twig' %}
{% block body %}

<div class="mws-panel grid_4">
                    
    <div class="mws-panel-header">
                    	<span>Datos del Cajero</span>
                    </div>
    <div class="mws-panel-body">
        <div class="mws-panel-toolbar bottom clearfix">
                        	<ul>
                                <li><a href="{{ path('administracion_entidades_cajero') }}" class="mws-ic-16 ic-arrow-left">Volver a la Lista</a></li>
                            	<li><a href="{{ path('administracion_entidades_cajero_edit', { 'id': entity.id }) }}" class="mws-ic-16 ic-pencil">Editar</a></li>                            	
                            </ul>
                        </div>
        <div class="mws-panel-content">
        
                            

<table class="mws-table">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Apellido</th>
            <td>{{ entity.apellido }}</td>
        </tr>
        <tr>
            <th>Nombres</th>
            <td>{{ entity.nombres }}</td>
        </tr>
        <tr>
            <th>Numero_documento</th>
            <td>{{ entity.numerodocumento }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>        
    </tbody>
</table>


        </div>
            </div>
    </div>
                          
                </div>
   
{% endblock %}
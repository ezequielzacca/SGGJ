{% extends 'GremioAbmBundle::layout.html.twig' %}
{% block body %}
<div class="mws-panel grid_4">
                    
    <div class="mws-panel-header">
                    	<span>Datos del Evento</span>
                    </div>
    <div class="mws-panel-body">
        <div class="mws-panel-toolbar bottom clearfix">
                        	<ul>
                                <li><a href="{{ path('administracion_entidades_evento') }}" class="mws-ic-16 ic-arrow-left">Volver a la Lista</a></li>
                            	<li><a href="{{ path('administracion_entidades_evento_edit', { 'id': entity.id }) }}" class="mws-ic-16 ic-pencil">Editar</a></li>                            	
                            </ul>
                        </div>
        <div class="mws-panel-content">
        
                            
<table class="mws-table">
    <tbody>
        {#<tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>#}
        
        <tr>
            <th>Razon Social</th>
            <td>{{ entity.razonsocial }}</td>
        </tr>
        <tr>
            <th>Fecha de Vencimiento</th>
            <td>{{ entity.fechaVencimiento is empty ? "" : entity.fechaVencimiento | date("d/m/y") }}</td>
        </tr>
        <tr>
            <th>Cuit</th>
            <td>{{ entity.cuit }}</td>
        </tr>
        <tr>
            <th>Telefono</th>
            <td>{{ entity.telefono }}</td>
        </tr>
        <tr>
            <th>Domicilio</th>
            <td>{{ entity.domicilio }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>
        
        
    </tbody>
</table>
            
            </div>
            </div>
    </div>
                          
                </div>


       
   
        
{% endblock %}
